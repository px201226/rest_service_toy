A,UAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,MAAA,KAAA,CAAA,IAAA,GAAA,GAAA;AACA,KALA,EAFA,CASA;;AACA,SAAA,MAAA,CAAA,QAAA,CAAA,oBAAA;AACA,GAzBA;AA2BA,EAAA,SA3BA,uBA2BA;AACA,SAAA,MAAA,CAAA,MAAA,CAAA,gBAAA,EAAA,IAAA;AACA,SAAA,MAAA,CAAA,MAAA,CAAA,iBAAA,EAAA,IAAA;AACA,GA9BA;AAgCA,EAAA,QAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,UAAA,KAAA,IAAA,CAAA,MAAA,KAAA,IAAA,EAAA,OAAA,MAAA,CAAA,KACA,OAAA,MAAA;AACA,KAJA;AAMA,IAAA,cANA,4BAMA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,gBAAA;AACA;AARA,GAhCA;AA2CA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,EADA,EACA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,mBAAA,EAAA,EAAA,EAAA,IAAA,CAAA,YAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,OAFA;AAGA,KALA;AAOA,IAAA,iBAPA,+BAOA;AACA,WAAA,OAAA,CAAA,IAAA;AACA,KATA;AAWA,IAAA,mBAXA,iCAWA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,YAAA,EAAA;AACA,QAAA,KAAA,EAAA,IADA;AAEA,QAAA,OAAA,EAAA,oBAFA;AAGA,QAAA,OAAA,EAAA,IAHA;AAIA,QAAA,OAAA,EAAA,IAJA;AAKA,QAAA,IAAA,EAAA,KAAA,IAAA,CAAA;AALA,OAAA;AAOA,KAnBA;AAqBA,IAAA,iBArBA,+BAqBA;AAAA;;AACA,UAAA,KAAA,IAAA,CAAA,MAAA,KAAA,KAAA,EAAA;AACA,aAAA,MAAA,CACA,QADA,CACA,YADA,EACA,KAAA,IAAA,CAAA,EADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,MAAA,GAAA,IAAA;AACA,SAJA,EAKA,KALA,CAKA;AAAA,iBAAA,EAAA;AAAA,SALA;AAMA,OAPA,MAOA;AACA,aAAA,MAAA,CACA,QADA,CACA,cADA,EACA,KAAA,IAAA,CAAA,EADA,EAEA,IAFA,CAEA;AAAA,iBAAA,MAAA,CAAA,IAAA,CAAA,MAAA,GAAA,KAAA;AAAA,SAFA,EAGA,KAHA,CAGA;AAAA,iBAAA,EAAA;AAAA,SAHA;AAIA;AACA;AAnCA;AA3CA,CAAA","sourcesContent":["<template>\r\n  <v-card>\r\n    <v-toolbar dark color=\"primary\">\r\n      <v-btn icon dark @click=\"onBackButtonClick\">\r\n        <v-icon>mdi-close</v-icon>\r\n      </v-btn>\r\n      <v-toolbar-title>뒤로가기</v-toolbar-title>\r\n      <v-spacer></v-spacer>\r\n    </v-toolbar>\r\n\r\n    <div class=\"pa-2\">\r\n      <v-row row>\r\n        <v-col class=\"flex-grow-0 flex-shrink-0\">\r\n          <v-avatar size=\"36px\">\r\n            <img src=\"@/assets/no_profile.gif\" alt=\"John\" />\r\n          </v-avatar>\r\n        </v-col>\r\n\r\n        <v-col class=\"\">\r\n          <v-row>\r\n            <div class=\"text--primary font-weight-bold\">\r\n              {{ post.userNickName }}\r\n            </div>\r\n          </v-row>\r\n          <v-row>\r\n            <div class=\"font-weight-light\">{{ post.modifyDate }}</div>\r\n          </v-row>\r\n        </v-col>\r\n        <div class=\"mr-2\" v-if=\"post.isWriter\">\r\n          <v-btn class=\"ml-auto\" icon>\r\n            <v-icon>mdi-pencil</v-icon>\r\n          </v-btn>\r\n          <v-btn class=\"ml-auto\" icon @click=\"onDeleteButtonClick\">\r\n            <v-icon>mdi-delete</v-icon>\r\n          </v-btn>\r\n        </div>\r\n      </v-row>\r\n\r\n      <div class=\"font-weight-regular ml-4 mt-3 mb-6\">\r\n        {{ post.content }}\r\n      </div>\r\n\r\n      <v-card-actions>\r\n        <v-btn icon :color=\"isLikeColor\" @click=\"onLikeButtonClick\">\r\n          <v-icon>mdi-heart</v-icon>\r\n        </v-btn>\r\n        <v-btn text color=\"\" class=\"ml-auto\">\r\n          좋아요 {{ post.likes }}개 | 댓글 {{ post.comments }}개\r\n        </v-btn>\r\n      </v-card-actions>\r\n    </div>\r\n\r\n    <v-divider />\r\n    <v-row class=\"pa-5 pb-0 \">\r\n      <v-textarea\r\n        label=\"댓글을 입력하세요\"\r\n        auto-grow\r\n        outlined\r\n        rows=\"1\"\r\n        row-height=\"5\"\r\n      ></v-textarea>\r\n      <v-btn class=\"mx-1 \" height=\"55\" depressed color=\"primary\">작성</v-btn>\r\n    </v-row>\r\n    <v-divider />\r\n\r\n    <comment\r\n      v-for=\"(comment, index) in getCommentList\"\r\n      :key=\"index\"\r\n      :id=\"comment.id\"\r\n      :nickName=\"comment.userNickName\"\r\n      :content=\"comment.content\"\r\n      :date=\"comment.modifyDate\"\r\n      :isLike=\"comment.isLike\"\r\n      :isWriter=\"comment.isWriter\"\r\n      class=\"my-1\"\r\n    />\r\n\r\n    <modal @pass=\"deleteEmit\" />\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nimport Modal from \"../components/Modal.vue\";\r\nimport BottomNavigator from \"./BottomNavigator.vue\";\r\nimport Comment from \"./Comment.vue\";\r\n\r\nexport default {\r\n  components: { BottomNavigator, Comment, Modal },\r\n  name: \"PostView\",\r\n  data() {\r\n    return {\r\n      post: \"\",\r\n    };\r\n  },\r\n  mounted() {\r\n    // 툴바, 네비게이션바 숨기기\r\n    this.$store.commit(\"APPBAR_DISPLAY\", false);\r\n    this.$store.commit(\"NAVIBAR_DISPLAY\", false);\r\n  },\r\n\r\n  created() {\r\n    // POST READ\r\n    this.$store\r\n      .dispatch(\"QUERY_GET_POST\", this.$route.params.id)\r\n      .then((res) => {\r\n        console.log(res);\r\n        this.post = res;\r\n      });\r\n\r\n    //COMMENT READ\r\n    this.$store.dispatch(\"QUERY_COMMENT_LIST\");\r\n  },\r\n\r\n  destroyed() {\r\n    this.$store.commit(\"APPBAR_DISPLAY\", true);\r\n    this.$store.commit(\"NAVIBAR_DISPLAY\", true);\r\n  },\r\n\r\n  computed: {\r\n    isLikeColor() {\r\n      if (this.post.isLike === true) return \"pink\";\r\n      else return \"grey\";\r\n    },\r\n\r\n    getCommentList() {\r\n      return this.$store.getters.GET_COMMENT_LIST;\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    deleteEmit(id) {\r\n      this.$store.dispatch(\"QUERY_DELETE_POST\", id).then(() => {\r\n        console.log(\"삭제\");\r\n      });\r\n    },\r\n\r\n    onBackButtonClick() {\r\n      this.$router.back();\r\n    },\r\n\r\n    onDeleteButtonClick() {\r\n      this.$store.commit(\"OPEN_MODAL\", {\r\n        title: \"확인\",\r\n        content: \"게시물을 정말로 삭제하시겠습니까?\",\r\n        option1: \"닫기\",\r\n        option2: \"삭제\",\r\n        data: this.post.id,\r\n      });\r\n    },\r\n\r\n    onLikeButtonClick() {\r\n      if (this.post.isLike === false) {\r\n        this.$store\r\n          .dispatch(\"QUERY_LIKE\", this.post.id)\r\n          .then((req) => {\r\n            this.post.isLike = true;\r\n          })\r\n          .catch(() => \"\");\r\n      } else {\r\n        this.$store\r\n          .dispatch(\"QUERY_UNLIKE\", this.post.id)\r\n          .then(() => (this.post.isLike = false))\r\n          .catch(() => \"\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}