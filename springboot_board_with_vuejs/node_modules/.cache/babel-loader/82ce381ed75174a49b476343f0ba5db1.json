,OAAA,EAAA,oBAFA;AAGA,QAAA,OAAA,EAAA,IAHA;AAIA,QAAA,OAAA,EAAA,IAJA;AAKA,QAAA,IAAA,EAAA,KAAA;AALA,OAAA;AAOA,KATA;AAWA,IAAA,iBAXA,+BAWA;AAAA;;AACA,UAAA,KAAA,IAAA,KAAA,KAAA,EAAA;AACA,aAAA,MAAA,CACA,QADA,CACA,YADA,EACA,KAAA,EADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,MAAA;AACA,SALA,EAMA,KANA,CAMA;AAAA,iBAAA,EAAA;AAAA,SANA;AAOA,OARA,MAQA;AACA,aAAA,MAAA,CACA,QADA,CACA,cADA,EACA,KAAA,EADA,EAEA,IAFA,CAEA,YAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,MAAA;AACA,SALA,EAMA,KANA,CAMA;AAAA,iBAAA,EAAA;AAAA,SANA;AAOA;AACA,KA7BA;AA+BA,IAAA,YA/BA,0BA+BA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,YAAA,KAAA,EAAA;AACA;AAjCA;AA7BA,CAAA","sourcesContent":["<template>\n  <v-card class=\"pa-2\" contextmenu=\"\" flat outlined>\n    <v-row row>\n      <v-col class=\"flex-grow-0 flex-shrink-0\">\n        <v-avatar size=\"36px\">\n          <img src=\"@/assets/no_profile.gif\" alt=\"John\" />\n        </v-avatar>\n      </v-col>\n\n      <v-col class=\"\">\n        <v-row>\n          <div class=\"text--primary font-weight-bold\">\n            {{ nickName }}\n          </div>\n        </v-row>\n        <v-row>\n          <div class=\"font-weight-light\">{{ date }}</div>\n        </v-row>\n      </v-col>\n      <div class=\"mr-2\" v-if=\"isWriter\">\n        <v-btn class=\"ml-auto\" icon>\n          <v-icon>mdi-pencil</v-icon>\n        </v-btn>\n        <v-btn class=\"ml-auto\" icon @click=\"onDeleteButtonClick\">\n          <v-icon>mdi-delete</v-icon>\n        </v-btn>\n      </div>\n    </v-row>\n\n    <div class=\"font-weight-regular ml-4 mt-3 mb-6\">\n      {{ content }}\n    </div>\n\n    <v-card-actions>\n      <v-btn icon :color=\"isLikeColor\" @click=\"onLikeButtonClick\">\n        <v-icon>mdi-heart</v-icon>\n      </v-btn>\n      <v-btn text color=\"\" class=\"ml-auto\" @click=\"openPostView\">\n        좋아요 {{ mlikes }}개 | 댓글 {{ comments }}개\n      </v-btn>\n    </v-card-actions>\n  </v-card>\n</template>\n\n<script>\nexport default {\n  name: \"PostItem\",\n  props: [\n    \"id\",\n    \"nickName\",\n    \"date\",\n    \"likes\",\n    \"comments\",\n    \"content\",\n    \"isLike\",\n    \"isWriter\",\n  ],\n  data() {\n    return {\n      like: this.isLike,\n      mlikes: this.likes,\n      expand: false,\n      expand2: false,\n    };\n  },\n  components: {},\n\n  computed: {\n    isLikeColor() {\n      if (this.like === true) return \"pink\";\n      else return \"grey\";\n    },\n  },\n\n  methods: {\n    onDeleteButtonClick() {\n      this.$store.commit(\"OPEN_MODAL\", {\n        title: \"확인\",\n        content: \"게시물을 정말로 삭제하시겠습니까?\",\n        option1: \"닫기\",\n        option2: \"삭제\",\n        data: this.id,\n      });\n    },\n\n    onLikeButtonClick() {\n      if (this.like === false) {\n        this.$store\n          .dispatch(\"QUERY_LIKE\", this.id)\n          .then((req) => {\n            this.like = true;\n            this.mlikes++;\n          })\n          .catch(() => \"\");\n      } else {\n        this.$store\n          .dispatch(\"QUERY_UNLIKE\", this.id)\n          .then(() => {\n            this.like = false;\n            this.mlikes--;\n          })\n          .catch(() => \"\");\n      }\n    },\n\n    openPostView() {\n      this.$router.push(\"/posts/\" + this.id);\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}