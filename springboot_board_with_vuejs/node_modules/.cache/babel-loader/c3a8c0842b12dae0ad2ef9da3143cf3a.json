   deleteAriticle() {\n      if (!this.isAuthenticated()) {\n        alert(\"권한이 없습니다\");\n        return;\n      }\n      this.$store.dispatch(\"QUERY_DELETE_POST\", this.getPostDetail.id);\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}