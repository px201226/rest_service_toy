quired\n            type=\"email\"\n            v-model=\"member.email\"\n          ></v-text-field>\n\n          <v-text-field\n            :rules=\"passwordRules\"\n            placeholder=\"Placeholder\"\n            outlined\n            prepend-icon=\"mdi-lock\"\n            label=\"패스워드\"\n            @keyup.enter=\"loginRequest\"\n            required\n            type=\"password\"\n            v-model=\"member.password\"\n          ></v-text-field>\n\n          <v-btn @click=\"loginRequest\" block color=\"primary\" outlined>\n            <v-icon left>mdi-login</v-icon>\n            로그인\n          </v-btn>\n\n          <v-btn\n            depressed\n            block\n            class=\"mt-3 text--right\"\n            color=\"primary\"\n            @click=\"onRegisterClick\"\n          >\n            회원가입\n          </v-btn>\n        </v-form>\n      </v-flex>\n    </v-layout>\n  </v-container>\n</template>\n\n<script>\nimport Modal from \"../components/Modal\";\n\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      member: {\n        email: \"\",\n        password: \"\",\n      },\n    };\n  },\n  computed: {\n    emailRules() {\n      return this.$store.state.common.emailRules;\n    },\n    loadingState() {\n      return this.$store.state.common.loadingState;\n    },\n    passwordRules() {\n      return this.$store.state.common.passwordRules;\n    },\n  },\n  components: {\n    Modal,\n  },\n  methods: {\n    loginRequest() {\n      if (this.$refs.form.validate()) {\n        this.$store.dispatch(\"REQUEST_LOGIN\", this.member);\n      }\n    },\n\n    onRegisterClick() {\n      this.$router.push(\"/register\");\n    },\n  },\n};\n</script>\n\n<style scoped>\n@media (min-width: 700px) {\n  .login {\n    width: 360px !important;\n  }\n}\n\n.login {\n  width: 80%;\n  background-color: white;\n  border-radius: 8px;\n  border: white 1px solid;\n  padding: 40px;\n}\n</style>\n"]}]}